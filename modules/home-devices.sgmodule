#!name=Home Devices
#!desc=After enabling the module, you may access the router configutation webpage by accessing http://router.com in your browser. The URL will always redirect to the gateway address in the current network.

[General]
force-http-engine-hosts=%APPEND% router.com, www.router.com, gateway.com, www.gateway.com, guard.com, www.guard.com

[MITM]
hostname = %INSERT% router.com, www.router.com, gateway.com, www.gateway.com, guard.com, www.guard.com

[URL Rewrite]
^https?:\/\/(|www\.)router\.com http://192.168.2.2 302
^https?:\/\/(|www\.)gateway\.com http://192.168.2.10:6166 302
^https?:\/\/(|www\.)guard\.com http://192.168.2.10:3000 302